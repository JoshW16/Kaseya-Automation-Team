        <Parameter xsi:type="StringParameter" name="ManagedFile" value="VSASharedFiles\Set-StaticIP.ps1" />
        <Parameter xsi:type="BooleanParameter" name="DeleteAfter" value="False" />
      </Statement>
      <If description="">
