        <Then>
          <Statement name="Execute Shell Command - Get Results to Variable" continueOnFail="false">
            <Parameter xsi:type="StringParameter" name="Parameter1" value="Powershell.exe -NoProfile -ExecutionPolicy Bypass -Command &quot;$PackageName= Get-WindowsPackage -Online | Where-Object {$_.PackageName -match 'KB5009624|17763.2350|20348.403'} | Select-Object -ExpandProperty PackageName; $FileName=&quot;&quot;&quot;C:\Windows\servicing\Packages\$PackageName.mum&quot;&quot;&quot;; (Get-Content $FileName) -replace 'permanent', 'removable' | Set-Content $FileName; Remove-WindowsPackage -PackageName $PackageName -Online -NoRestart&quot;" />
